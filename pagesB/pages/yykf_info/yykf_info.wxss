@import '/common/commom.wxss';  /*引用公共样式文件*/
.swiper-tab{
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background-color: white;
}
.swiper-tab-list{  
  font-size: 30rpx;
  display: inline-block;
  width: 50%;
  color: #777777;
}
.on{ 
  font-size: 18px;
  border-bottom: 6rpx solid #5677fc;
  font-weight: bold;
}
.swiper-box{ 
  display: block;
  height: 100%; 
  width: 100%; 
  overflow: hidden; 
  background-color: white;
}
.swiper-items{
  height: 100%;
}
.scroll-views{
  height: 100%;
}
.form_show_border_in{
  padding:20rpx 0rpx;
  height:20rpx;
  line-height:20rpx;
}
.form_show_left{
  float:left;
  font-size:3.5vw;
  height:20rpx;
  color: #afadad;
}
.form_show_right{
  float:right;
  height:20rpx;
  width:85%;
}
.form_show_right_text{
  font-size:3.5vw;
}
.form_info_bot1{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 7vh;
  border-top: 1px solid #e6e6e6;
  background-color: white;
}
.title-right-span{
 float:right;
 color: #5677fc;
 background-color: #e8e8e8;
 border-radius: 10rpx;
 padding: 0 10rpx;
}
.form_info_bot_td2_tz{
  width:100%;
  text-align:center;
  line-height: 7vh;
  font-size: 4vw;
  background-color:#5677FC;
  color:white;
}
/*操作记录样式*/
.weui-cell-third{
  background:#fff ;
}
.weui-cell-list{
  background:#fff ;
  margin: 10rpx 40rpx 10rpx 40rpx;
}
.weui-cell-line{
  margin-left: 10rpx;
  border-left: 2rpx solid #ddd;
  height: 130rpx;
  background:#fff ;
}
.weui-cell-circle{
  border: 2rpx solid #000;
  border-radius: 10rpx;
  width: 20rpx;
  height: 20rpx;
  border-color: green;
}
.weui-cell-name{
  float: left;
  font-size: 28rpx;
  padding-left: 15rpx;
  /*width: 170rpx;*/
}
.weui-cell-time{
  font-size: 28rpx;
  height: 46rpx;
  /*margin-left: 170rpx;*/
}
.weui-cell-event{
  padding-top: 10rpx;
  padding-left: 30rpx;
  font-size: 26rpx;
}
/*操作记录样式结束*/