@import '/common/commom.wxss';  /*引用公共样式文件*/
.swiper-items{
  height: 100%;
}
.scroll-views{
  height: 100%;
}
.service-list{
  padding: 0 20rpx;
}
.service-item{
	display: flex;
	padding: 14rpx; 
  background-color: white;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}
.service-text{
	flex: 1;
	margin-left: 20rpx;
	width: 100%;
}
.service-text .service-tite{
	color: #333;
	width: 100%;
	font-size: 32rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.form_info_bot1{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 7vh;
  border-top: 1px solid #e6e6e6;
  background-color: white;
}
.zd_tite_MQ_right{
  font-size: 30rpx;
  width: 100%;
  padding-right: 20rpx;
  text-align: right;
  font-weight: bold;
}
.zd_tite_MX_right{
  font-size: 30rpx;
  width: 100%;
  padding-right: 20rpx;
  text-align: right;
  color: #c0c0c0;
}
.zd_tite_MX_left{
  color: #c0c0c0;
	width: 100%;
	font-size: 30rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 7vh;
  line-height: 7vh;
  align-items: center;
}
.form_info_bot_td2_l{
  padding-left: 40rpx;
  width:60%;
  text-align:left;
  line-height: 7vh;
  font-size: 4vw;
  color:#000000;
}
.form_info_bot_td2_r{
  width:40%;
  text-align:center;
  line-height: 7vh;
  font-size: 4vw;
  background-color:#5677FC;
  color:white;
}