/*@import '/common/commom.wxss';*/ /*引用公共样式文件*/
.swiper-tab{
    width: 100%;
    text-align: center;
    line-height: 80rpx;
    background-color: white;
  }
  .swiper-tab-list{  
    font-size: 30rpx;
    display: inline-block;
    width: 33%;
    color: #777777;
  }
  .on{ 
    /*color:#5677fc;*/
    /*border-bottom: 4rpx solid #5677fc;*/
    font-size: 18px;
    border-bottom: 6rpx solid #5677fc;
    font-weight: bold;
    color:#5677fc;
  }
  .swiper-box{ 
    display: block;
    height: 100%; 
    width: 100%; 
    overflow: hidden; 
    background-color:white;
  }
  .swiper-items{
    height: 100%;
    background-color: #fafafa;
  }
  .scroll-views{
    height: 100%;
  }
  .service-list{
      padding: 0 20rpx;
  }
  .service-item{
      display: flex;
      padding: 14rpx 0; 
    /*border-bottom: 1rpx solid #e8e8e8;*/
    background-color: white;
    border-radius: 16rpx;
    margin-bottom: 20rpx;
  }
  .service-text{
      flex: 1;
      margin-left: 20rpx;
      width: 100%;
  }
  .service-text .service-tite{
      color: #333;
      width: 100%;
      font-size: 32rpx;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
  }
  .service-tite-left{
    float: left;
    /*
    color:#1E90FF;*/
    color:white;
    background-color: #5677fc;
      width: 45%;
      font-size: 28rpx;
      white-space: nowrap;
      overflow: hidden;
    text-overflow: ellipsis;
    border: 2rpx solid #1E90FF;
    border-radius: 10rpx;
    text-align: center;
    padding: 4rpx 0;
    margin-right: 40rpx;
  }
  .service-tite-right{
    float: right;
    color:white;
    background-color: #5677fc;
      width: 45%;
      font-size: 28rpx;
      white-space: nowrap;
      overflow: hidden;
    text-overflow: ellipsis;
    border-radius: 10rpx;
    text-align: center;
    padding: 4rpx 0;
    /*margin-right: 40rpx;*/
  }
  .span-style-cz{
    background-color:#2bb972;
    padding: 4rpx 8rpx;
    font-size: 24rpx;
    border-radius: 8rpx;
    color: white;
  }
  .nav{
    width: 100%;
    overflow: hidden;
    position: fixed;
    top:0;
    left: 0;
    z-index: 10;
    background:#5677fc;
  }
  .title_icon{
    position:absolute;
    bottom: 20rpx;
    left: 14rpx;
    display: flex;
    z-index: 20;
  }
  /*总菜单容器*/
  .menu {
    /*
    display: block;
    height: 74rpx;
    border-bottom: 2rpx solid #d2d2d2; 
    */
    display: block;
    height: 70rpx;
  }
  /*一级菜单*/
  .menu dt {
    font-size: 15px;
    float:left;
    width: 100%;
    /*width: 24.7%;*/
    /*width: 33%;*/
    height: 70rpx;
    border-right: 2rpx solid #d2d2d2;
    border-bottom: 2rpx solid #d2d2d2; 
    text-align: center;
    background-color: white;
    color: #5a5a5a;
    line-height: 70rpx;
    border-top: 2rpx solid #d2d2d2; 
  }
  /*二级菜单外部容器样式*/
  .menu dd{ 
    position: absolute;
    width: 100%;
    top:280rpx;
    left:0;
    z-index:999;
  }
  /*二级菜单普通样式*/
  .menu li{
    font-size: 28rpx; 
    line-height: 68rpx;
    color: #575757;
    height: 68rpx;
    display: block;
    padding-left: 16rpx;
    background-color: #fff;
    border-top: 2rpx solid #dbdbdb;
  }
  /* 显示与隐藏 */
  .show {
    display: block;
  }
  .hidden {
    display: none;
  }
  /*二级菜单高亮样式*/
  .menu li.highlight{
    background-color: #f4f4f4;
    color:#5677fc
  }
  /*筛选框样式开始*/
  .tr {
    display: flex;
    width: 100%;
    /*justify-content: center;*/
    justify-content: left;
    height: 5vh;
    line-height: 5vh;
    align-items: center;
  }
  .form_info_bot_td2_r{
    width:25%;
    text-align:center;
    line-height: 5vh;
    font-size: 3.5vw;
    background-color:#e8e8e8;
    /*color:white;*/
    border-radius: 10rpx;
  }
  .form_info_bot_td2_rq{
    width:33%;
    text-align:center;
    line-height: 5vh;
    font-size: 3.5vw;
    background-color:#e8e8e8;
    border-radius: 10rpx;
  }
  .form_info_bot_td2_date{
    width:50%;
    padding: 14rpx 0;
    text-align:center;
    line-height: 5vh;
    font-size: 3.5vw;
    background-color:#e8e8e8;
    border-radius: 10rpx;
  }
  .showBorder{
    border: 4rpx solid #000000;
    color:#5677fc;
    font-weight: bold;
  }
  .service-tite-left{
    float: left;
    /*color:#1E90FF;*/
    width: 50%;
    font-size: 28rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    /*border: 2rpx solid #1E90FF;*/
    text-align: center;
    padding: 4rpx 20rpx;
  }
  /*筛选框样式开始*/
  .form_line{
    background-color:#fafafa;
    border:0;
    clear:both;
    height:20rpx;
  }
  .form_info_bot_button_l{
    width:50%;
    text-align:center;
    line-height: 6vh;
    font-size: 4vw;
    border-top: 2rpx solid #c0c0c0;
    /*
    background-color:#5677FC;
    color:white;*/
  }
  .form_info_bot_button_r{
    width:50%;
    text-align:center;
    line-height: 6vh;
    font-size: 4vw;
    background-color:#5677FC;
    color:white;
    border-top: 2rpx solid #c0c0c0;
  }
  .curtain{
    width:100%;
    height:100%;
    position:fixed;
    background-color: black;
    z-index:9999;
    top:320rpx;
    left:0;
    opacity:0.4;
    overflow-y: hidden;
  }
  /*以下为弹出框样式*/
  .toast-box {
    width: 100%;
    height: 100%;
    opacity: 1;
    position: fixed;
    top: 0px;
    left: 0px;
  }
  .toastbg {
    opacity: 0.2;
    background-color: black;
    position: absolute;
    width: 100%;
    min-height: 100vh;
  }
  .showToast {
    position: absolute;
    opacity: 1;
    width: 70%;
    margin-left: 15%;
    margin-top: 40%;
  }
  .toast-title {
    text-align: center;
    padding-left: 5%;
    background-color: #5677fc;
    color: white;
    padding-top: 2vh;
    padding-bottom: 2vh;
    border-top-right-radius: 16rpx;
    border-top-left-radius: 16rpx;
  }
  .toast-main {
    padding-top: 2vh;
    padding-bottom: 2vh;
    background-color: white;
    text-align: center;
  }
  .toast-input {
    margin-left: 5%;
    margin-right: 5%;
    border: 1px solid #ddd;
    padding-left: 2vh;
    padding-right: 2vh;
    padding-top: 1vh;
    padding-bottom: 1vh;
  }
  .toast-button {
    display: flex;
  }
  .cancelButton {
    width: 50%;
    border-top: 2rpx solid #ddd;
    border-right: 2rpx solid #ddd;
  }
  .confirmButton {
    width: 50%;
    border-top: 2rpx solid #ddd;
  }
  .cancelButton button {
    width: 100%;
    background-color: white;
    color:#7ebcf7;
    border-radius: 0px;
    border-bottom-left-radius: 16rpx;
  }
  .confirmButton button{
    width: 100%;
    background-color: white;
    color:#7ebcf7;
    border-radius: 0px;
    border-bottom-right-radius: 16rpx;
  }
  /*以上为弹出框样式*/