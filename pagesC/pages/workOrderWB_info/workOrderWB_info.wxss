@import '/common/commom.wxss';  /*引用公共样式文件*/
.swiper-tab{
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background-color: white;
}
.swiper-tab-list{  
  font-size: 30rpx;
  display: inline-block;
  width: 50%;
  color: #777777;
}
.on{ 
  /*color:#5677fc;*/
  /*border-bottom: 4rpx solid #5677fc;*/
  font-size: 18px;
  border-bottom: 6rpx solid #5677fc;
  font-weight: bold;
}
.swiper-box{ 
  display: block;
  height: 100%; 
  width: 100%; 
  overflow: hidden; 
  background-color: white;
}
.swiper-items{
  height: 100%;
}
.scroll-views{
  height: 100%;
}


.weui-cell-third{
  background:#fff ;
}
.weui-cell-list{
  background:#fff ;
  margin: 10rpx 40rpx 10rpx 40rpx;
}
.weui-cell-line{
  margin-left: 10rpx;
  border-left: 2rpx solid #ddd;
  height: 130rpx;
  background:#fff ;
}
.weui-cell-circle{
  border: 2rpx solid #000;
  border-radius: 10rpx;
  width: 20rpx;
  height: 20rpx;
  border-color: green;
}
.weui-cell-name{
  float: left;
  font-size: 28rpx;
  padding-left: 15rpx;
  /*width: 170rpx;*/
}
.weui-cell-time{
  font-size: 28rpx;
  height: 46rpx;
  /*margin-left: 170rpx;*/
}
.weui-cell-event{
  padding-top: 10rpx;
  padding-left: 30rpx;
  font-size: 26rpx;
}
.img{
  display: inline-block;
}
.pic {
float:left;
position:relative;
margin-right:18rpx;
margin-bottom:18rpx;
width: 192rpx;
height: 192rpx;
}
.delete-btn{
  position: absolute;
  top: 0;
  right: 0;
}
.weui-uploader{
  padding: 10rpx;
  background-color: white;
}
.pic image{
  width: 100%;
  height: 100%;
}
.button {
  width: 100%;
  margin-top: 30rpx;
}
.middle-item-img{
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx 0 0 30rpx;
}
.form_title_border_in{
  padding: 0;
  height: 90rpx;
  line-height: 90rpx;
}
.form_title_left{
  float: left;
  text-align: right;
  font-size: 16px;
  font-weight: bold;
  width: 60%;
}
.tr{
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}
.index_mine_td{
  width:33%;
  /*padding:20rpx;*/
}
.index_mine_td_d1{
  text-align:center;
  border: 2rpx solid #f5f5f5;
  /*background-color:#f5f5f5;
  border-radius:10rpx;*/
  padding:20rpx;
  height:200rpx;
}
.index_mine_td_d1 image{
  width:80rpx;
  margin:10rpx;
  height:80rpx;
}
.index_mine_td_d1 view{
  font-size:3.5vw;
  margin:10rpx;
}
.list_left {
  font-size:30rpx;
}
.list_right {
  font-size:30rpx;
