@import '/common/commom.wxss';  /*引用公共样式文件*/
.service-list{
  padding: 0 20rpx;
}
.service-item{
  display: flex;
  padding: 20rpx 0; 
  border-bottom: 1rpx solid #e8e8e8;
}
.service-img{
  width: 50rpx;
  height: 50rpx;
}
.service-item-image{
  width: 50rpx;
  height: 50rpx;
}
.service-text{
  flex: 1;
  /*
  margin-left: 20rpx;*/
  width: 100%;
}
.service-text .service-tite{
  color: #333;
  width: 100%;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-left: 10rpx;
  height: 50rpx;
}
.service-tite-left{
  line-height: 50rpx;
}
.service-left-img{
  width: 40rpx;
  height: 30rpx;
  padding-left:20rpx;
}
.swiper-items{
  height: 100%;
  background-color: white;
}
.scroll-views{
  height: 100%;
}
.notLink-span{
  color:#ff0000;
  float: right;
  margin-right: 80rpx;
}
.Link-span{
  color: #04BE02;
  float: right;
  margin-right: 80rpx;
}