/* pages/openLock_list/openLock_list.wxss */
@import '/common/commom.wxss';  /*引用公共样式文件*/
.service-list{
  padding: 0 20rpx;
}
.service-item{
  display: flex;
  padding: 14rpx 0; 
  border-bottom: 1rpx solid #e8e8e8;
}
.service-text{
  flex: 1;
  margin-left: 20rpx;
  width: 100%;
}
.service-text .service-tite{
  color: #333;
  width: 100%;
  font-size: 32rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.service-city{
  height: 40rpx;
  /*margin: 10rpx 0;*/
  font-size: 28rpx;
  color: #C0C0C0;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-family:LiSu,STLiti,"Microsoft YaHei",微软雅黑,"MicrosoftJhengHei",华文细黑,STHeiti,MingLiu;
}
.swiper-items{
  height: 100%;
}
.scroll-views{
  height: 100%;
  background-color: white;
}
.date_style{
  margin: 20rpx;
  background-color: white;
  width: 65%;
  padding: 4rpx 20rpx;
  border-radius: 20rpx;
}
.form_date_right_text{
  border: none;
  font-size: 4vw;
  line-height: 50rpx;
  outline: medium;
  text-align: center;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 5vh;
  line-height: 5vh;
  align-items: center;
}
.form_info_bot_td2_r{
  width:23%;
  text-align:center;
  line-height: 5vh;
  font-size: 3.5vw;
  background-color:#e8e8e8;
  /*color:white;*/
  border-radius: 10rpx;
}
.showBorder{
  border: 4rpx solid #000000;
}
.button_left{
  font-size: 14px;
  width: 20%;
  background-color: #f7982a;
  text-align: center;
  border-radius: 10rpx;
  color: #ffffff;
  margin: 10rpx;
  height: 72rpx;
  line-height: 72rpx;  
}