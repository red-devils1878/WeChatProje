@import '/common/commom.wxss';  /*引用公共样式文件*/
.button_left{
  font-size: 14px;
  width: 20%;
  background-color: #3fafa6;
  text-align: center;
  border-radius: 10rpx;
  color: #ffffff;
  padding: 2rpx 10rpx;
  margin-left: 10rpx;
}
.table_center{
  display: flex;
  height: 50rpx;
  width: 80%;
}
.table_center_text {
  border: none;
  font-size: 4vw;
  line-height: 50rpx;
  outline: medium;
  text-align: right;
  width: 100%;
}
.form_table_detail_right_text{
  border: none;
  font-size: 4vw;
  line-height: 50rpx;
  outline: medium;
  text-align: right;
  width: 100%;
  color: #c0c0c0;
}
.curtain{
  width:100%;
  height:100%;
  position:fixed;
  background-color: black;
  z-index:9999;
  top:0;
  left:0;
  opacity:0.4;
}