.swiper-tab{
    width: 100%;
    text-align: center;
    line-height: 80rpx;
    background-color: white;
  }
  .swiper-tab-list{  
    font-size: 30rpx;
    display: inline-block;
    width: 33.33%;
    color: #777777;
  }
  .on{ 
    font-size: 18px;
    border-bottom: 6rpx solid #5677fc;
    font-weight: bold;
  }
  .swiper-box{ 
    display: block;
    height: 100%; 
    width: 100%; 
    overflow: hidden; 
    background-color: white;
  }
  .swiper-items{
    height: 100%;
  }
  .scroll-views{
    height: 100%;
  }
  .index_build_border{
    /*padding-top:10rpx;*/
    float:left;
    width:100%;
    /*border: 1px solid rgb(140,150,170);*/
    margin-top:20rpx;
  }
  .index_build_border_t1{
    padding:0 10rpx;
    font-size:4vw;
    font-weight:bold;
    width: 100%;
  }
  .index_build_icon_border{
    float:left;
    width:30%;
    /*border: 1px solid rgb(140,150,170);*/
    box-shadow: 0px 10rpx 10rpx #6666;
    margin:10rpx;
  }
  .index_build_icon_t1{
    font-size: 3vw;
    height: 80rpx;
    background: linear-gradient(to right,rgb(246,122,36),rgb(252,246,93));
    text-align: center;
    line-height: 80rpx;
    border-bottom-right-radius: 200rpx 30rpx;
    border-bottom-left-radius: 200rpx 30rpx;
    color: white;
  }
  .index_build_icon_t2{
    text-align: center;
    margin: 30rpx 0 50rpx 0;
    font-size: 3vw;
  }
  .index_build_icon_t3{
    text-align: center;
    color: rgb(239,180,136);
    margin: 20rpx;
    font-size:3vw;
  }
  .nav{
    width: 100%;
    overflow: hidden;
    position: fixed;
    top:0;
    left: 0;
    z-index: 10;
    background:#5677fc;
  }
  .title_icon{
    position:absolute;
    bottom: 20rpx;
    left: 14rpx;
    display: flex;
    z-index: 20;
  }
  /*总菜单容器*/
  .menu {
    display: block;
    height: 70rpx;
  }
  /*一级菜单*/
  .menu dt {
    font-size: 15px;
    float:left;
    width: 24.7%;
    /*width: 33%;*/
    height: 70rpx;
    border-right: 2rpx solid #d2d2d2;
    border-bottom: 2rpx solid #d2d2d2; 
    text-align: center;
    /*background-color: #f4f4f4; */
    background-color: white;
    color: #5a5a5a;
    line-height: 70rpx;
    border-top: 2rpx solid #d2d2d2; 
  }
  /*二级菜单外部容器样式*/
  .menu dd{ 
    position: absolute;
    width: 100%;
    top:280rpx;
    left:0;
    z-index:999;
  }
  /*二级菜单普通样式*/
  .menu li{
     font-size: 28rpx; 
     line-height: 68rpx;
     color: #575757;
     height: 68rpx;
     display: block;
     padding-left: 16rpx;
     background-color: #fff;
     border-top: 2rpx solid #dbdbdb;
  }
  /* 显示与隐藏 */
  .show {
    display: block;
  }
  .hidden {
    display: none;
  }
  /*二级菜单高亮样式*/
  .menu li.highlight{
     background-color: #f4f4f4;
     color:#5677fc
  }
  .index_build_icon_borderN_1{
    float:left;
    width:30%;
    /*margin:10rpx;*/
    margin:10rpx 8rpx;
    background-color: white;
    border-radius: 10rpx;
    border: 2rpx solid #5677fc;
  }
  .index_build_icon_borderN_2{
    float:left;
    width:30%;
    /*margin:10rpx;*/
    margin:10rpx 8rpx;
    background-color:#9ae2fa;
    border-radius: 10rpx;
    border: 2rpx solid #5677fc;
  }
  .index_build_icon_borderN_3{
    float:left;
    width:30%;
    /*margin:10rpx;*/
    margin:10rpx 8rpx;
    background-color:#ffc0cb;
    border-radius: 10rpx;
    border: 2rpx solid #5677fc;
  }
  .index_build_icon_t4{
    margin: 10rpx 0 70rpx 0;
    font-size: 3.5vw;
    font-weight: bold;
    padding-left: 20rpx;
  }
  .floor_title{
    font-size: 26rpx;
    color:white;
    background-color:#b8b8b8;
    border-radius: 10rpx;
    padding: 6rpx 10rpx;
  }
  /*以下为弹出框样式*/
  .toast-box {
    width: 100%;
    height: 100%;
    opacity: 1;
    position: fixed;
    top: 0px;
    left: 0px;
  }
  .toastbg {
    opacity: 0.2;
    background-color: black;
    position: absolute;
    width: 100%;
    min-height: 100vh;
  }
  .showToast {
    position: absolute;
    opacity: 1;
    width: 70%;
    margin-left: 15%;
    margin-top: 40%;
  }
  .toast-title {
    text-align: center;
    padding-left: 5%;
    background-color: #5677fc;
    color: white;
    padding-top: 2vh;
    padding-bottom: 2vh;
    border-top-right-radius: 16rpx;
    border-top-left-radius: 16rpx;
  }
  .toast-main {
    padding-top: 2vh;
    padding-bottom: 2vh;
    background-color: white;
    text-align: center;
  }
  .toast-input {
    margin-left: 5%;
    margin-right: 5%;
    border: 1px solid #ddd;
    padding-left: 2vh;
    padding-right: 2vh;
    padding-top: 1vh;
    padding-bottom: 1vh;
  }
  .toast-button {
    display: flex;
  }
  .cancelButton {
    width: 50%;
    border-top: 1rpx solid #ddd;
    border-right: 1rpx solid #ddd;
  }
  .confirmButton {
    width: 50%;
    border-top: 1rpx solid #ddd;
  }
  .cancelButton button {
    width: 100%;
    background-color: white;
    color:#7ebcf7;
    border-radius: 0px;
    border-bottom-left-radius: 16rpx;
  }
  .confirmButton button{
    width: 100%;
    background-color: white;
    color:#7ebcf7;
    border-radius: 0px;
    border-bottom-right-radius: 16rpx;
  }
  /*以上为弹出框样式*/
