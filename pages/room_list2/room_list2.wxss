  margin:0 20rpx;
}
.form_table_border_in{
  padding:20rpx 0rpx;
  height:50rpx;
  line-height:50rpx;
}
.form_table_left{
  float:left;
  font-size:4vw;
  height:50rpx;
  width:40%;
}
.form_table_right{
  float:right;
  height:50rpx;
  width:60%;
}
.form_table_right_text{
  border:2rpx solid #c0c0c0;
  padding-right: 20rpx;
