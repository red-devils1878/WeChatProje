.left {
  width: 15%;
  float: left;
  border-right: 1px solid #666;
  /*height: 100%;*/
  height: calc(100vh - calc(100vw/1080*520));
  box-sizing: border-box;
  background-color:#fafafa
}
.left view{
  border-bottom: 1rpx solid #666;
  /*background: #fff;*/
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: relative;
  background:#fafafa;
}
.left view::before{
  content: '';
  /* background: #fff;
  border-left: 8rpx; */
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  width: 8rpx;
}
.left view.active::before{
  background: #5677fc;
  color: #5677fc;
  font-weight: bold;
}
.right{
  float: right;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 85%;
  padding: 0 10rpx;
  /* background: #efefefef; */
  box-sizing: border-box;
  height: calc(100vh - calc(100vw/1080*520));
  /*height: 100%;*/
}
.right .title{
  height: 40rpx;
  line-height: 40rpx;
}
.index_build_background{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  background-color:#fafafa;
}
.ActionSheet{
  margin-bottom: 100rpx;
}